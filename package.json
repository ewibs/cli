    "prepare": "rm -rf lib && tsc && chmod +x lib/cli.js"
  },
  "devDependencies": {
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@ewibs/assembly": "^0.2.5",
    "rxjs": "^7.5.5",
    "ts-node": "^10.8.1"
  },
  "bin": {
    "ewibs": "./lib/cli.js"
  }
}
