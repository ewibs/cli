{
  "name": "@ewibs/cli",
  "version": "0.0.2",
  "description": "CLI for the ewibs system",
  "type": "commonjs",
  "repository": {
    "url": "https://github.com/ewibs/cli"
  },
  "author": "Simon Rothert <puresamari@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
